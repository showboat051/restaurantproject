    <div form="comments"></div>
    <div id="map"></div>
     <script src="https://maps.googleapis.com/maps/api/js?callback=initMap"async defer></script>
        <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 32.7812034, lng: -96.8064713},
          zoom: 8
       });
      }
    </script>

     

